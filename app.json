{
  "expo": {
    "name": "appTrabajo",
    "slug": "appTrabajo",
    "scheme": "solucionesya",
    "linking": {
      "prefixes": [
        "solucionesya://"
      ],
      "config": {
        "screens": {
          "pago-exitoso": "pago-exitoso"
        }
      }
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/icon.png",
      "assets/splash-icon.png",
      "assets/adaptive-icon.png",
      "assets/favicon.png"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos acceder a tu ubicaci√≥n para mostrarla en el mapa"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alex_6775.appTrabajo",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "aea4aff9-1e66-4a9c-a2e7-17402b7bc1f1"
      }
    },
    "owner": "perezzz2003"
  }
}
